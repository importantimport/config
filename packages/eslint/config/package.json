{
  "name": "@importantimport/eslint-config",
  "type": "module",
  "version": "0.2.0",
  "description": "An ESLint Flat Config for !mportantImport.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/importantimport/config.git",
    "directory": "packages/eslint/config"
  },
  "bugs": "https://github.com/importantimport/config/issues",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "config"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js",
      "default": "./dist/utils/*.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch"
  },
  "peerDependencies": {
    "@importantimport/eslint-config-react": "workspace:*",
    "@importantimport/eslint-config-ts": "workspace:*",
    "@importantimport/eslint-config-vanilla-extract": "workspace:*",
    "eslint": "^8.39.0"
  },
  "peerDependenciesMeta": {
    "@importantimport/eslint-config-react": {
      "optional": true
    },
    "@importantimport/eslint-config-ts": {
      "optional": true
    },
    "@importantimport/eslint-config-vanilla-extract": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^3.2.1",
    "@eslint/js": "^8.57.0",
    "@stylistic/eslint-plugin-js": "^0.0.6",
    "defu": "^6.1.4",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-antfu": "1.0.0-beta.8",
    "eslint-plugin-import": "npm:eslint-plugin-i@2.27.5-4",
    "eslint-plugin-jsonc": "^2.15.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-perfectionist": "^1.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-regexp": "^1.15.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "globals": "^13.24.0",
    "jsonc-eslint-parser": "^2.4.0"
  },
  "devDependencies": {
    "@importantimport/eslint-config-react": "workspace:*",
    "@importantimport/eslint-config-ts": "workspace:*",
    "@importantimport/eslint-config-vanilla-extract": "workspace:*",
    "@types/eslint": "^8.56.7",
    "eslint": "^8.57.0"
  }
}
